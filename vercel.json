{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.maidsflow.com/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/admin/index.html"
    },
    {
      "src": "/company/(.*)",
      "dest": "/company/index.html"
    },
    {
      "src": "/professional/(.*)",
      "dest": "/professional/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
